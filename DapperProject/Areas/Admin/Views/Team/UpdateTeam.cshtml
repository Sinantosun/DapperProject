@model ResultTeamByIdDto
@{
    ViewData["Title"] = "UpdateTeam";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}


<br />
<div class="col-lg-12 grid-margin stretch-card mt-5">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Yeni ekip arkadaşı ekleme sayfası</h4>
            <small class="card-description ">
                Bu alanda yeni ekip arkadaşı ekleme işlemi gerçekleştirebilirsiniz.
            </small>
            <br />
            <br />
            <form method="post">
                <input type="hidden" asp-for="TeamId" class="form-control" />
                <label>Ad Soyad</label>
                <input type="text" asp-for="NameSurname" class="form-control" />
                <br />
                <label>Unvan</label>
                <input type="text" asp-for="Title" class="form-control" />
                <br />
                <label>Linkedin Url </label>
                <input type="text" asp-for="LinkedinURL" class="form-control" />
                <br />
                <label>Instagram Url </label>
                <input type="text" asp-for="InstagramURL" class="form-control" />
                <br />
                <label>Görsel</label>
                <input type="text" asp-for="ImageUrl" class="form-control" />
                <br />
                <button class="btn btn-outline-primary">Ekle</button>
            </form>

        </div>
    </div>
</div>









